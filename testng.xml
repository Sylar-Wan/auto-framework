<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd"><!--parallet indicator if need to use multiple thread, fill 'methods' means use,thread-count means the count of thread. If parallet = false, means do not use multiple thread.--><suite verbose="0" name="Base FT" parallel="methods" thread-count="1">    <parameter name="report.config" value="src/main/resources/config/report/extent-config.xml"/>    <parameter name="system.info" value="Framework.MySystemInfo"/>    <test name="BasePlatform" preserve-order="false">            <parameter name="p1" value="test"/>        <classes>            <class name="testCase.signinSandbox">                 <methods>                     <include name="TestScript"/>                 </methods>            </class> <!-- testCase.signinSandbox -->        </classes>    </test> <!-- BasePlatform -->    <listeners>        <!--Test Report Listener-->        <listener class-name="com.vimalselvam.testng.listener.ExtentTestNgFormatter"/>    </listeners></suite> <!-- Base FT -->